+++ b/mediatek/mt6765/device.mk
@@ -1160,3 +1160,5 @@ $(call inherit-product-if-exists, vendor/mediatek/mt6765/device-vendor.mk)

 # .
 -include vendor/tinno/common/hardware/interface/FactoryServer/configs.mk
+#  add.
+-include vendor/tinno/common/hardware/interface/test/configs.mk
diff --git a/mediatek/vendor/common/BoardConfig.mk b/mediatek/vendor/common/BoardConfig.mk
index 996a40a..24a4569 100644
--- a/mediatek/vendor/common/BoardConfig.mk
+++ b/mediatek/vendor/common/BoardConfig.mk
@@ -341,3 +341,7 @@ endif

 #  add.
